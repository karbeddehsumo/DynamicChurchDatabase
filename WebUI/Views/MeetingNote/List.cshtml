@model IEnumerable<Domain.meetingnote>

@{
    ViewBag.Title = "List";
    
}
<div id="TopPane">

</div>
<h2>Agenda List</h2>

@if (@ViewBag.RecordCount > 0)
{
<table>
    <tr>
        <th>
            Agenda
        </th>
        <th>
            Note
        </th>
        <th>
           Date
        </th>
        <th>
            Status
        </th>
        <th>
            Entered By
        </th>
        <th>
            Date Entered
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MeetingAgendaID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Note)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NoteDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Status)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EnteredBy)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateEntered)
        </td>
                <td>
                     @Ajax.ActionLink("Edit", "Edit", "MeetingNote", new {MeetingNoteID=item.meetingNoteID}, new AjaxOptions { InsertionMode = InsertionMode.Replace, UpdateTargetId = "TopPane" })
                </td>
                <td>
                    @Ajax.ActionLink("Delete", "Delete", "MeetingNote", new {MeetingNoteID=item.meetingNoteID}, new AjaxOptions { InsertionMode = InsertionMode.Replace, UpdateTargetId = "TopPane" })
                </td>

    </tr>
}

</table>
    }
else
{
 <p>No attendance added yet!</p>   
}
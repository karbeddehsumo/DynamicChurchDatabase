@model IEnumerable<Domain.calendar>

<div class="cycle-slideshow">
    @foreach(var c in Model)
    {
        <a  href="@Url.Action("DisplayBannerCalendarEvent", "Calendar",new {calendarID=@c.calendarID})" title="@c.Description"><img alt="Image 1" width="200" src="@Url.Action("GetImageByID", "Picture",new {PictureID=@c.PictureID})" /></a>
    }


        </div>

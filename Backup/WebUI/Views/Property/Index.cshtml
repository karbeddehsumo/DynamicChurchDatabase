@model IEnumerable<Domain.property>

@{
    ViewBag.Title = "Index";
}



<h2 class="center-text">Property Center</h2>
<br />

    <div>
        @Ajax.ActionLink("Add Property Category", "Create", "Property", new {}, new AjaxOptions { InsertionMode = InsertionMode.Replace, UpdateTargetId = "RightPane" })
        @Ajax.ActionLink("View Property category", "Details", "Property", new {}, new AjaxOptions { InsertionMode = InsertionMode.Replace, UpdateTargetId = "RightPane" })
    </div>

<div id="RightPane">
@Html.Action("List", "PropertyInventory", new {bDate = @ViewBag.BeginDate, eDate = @ViewBag.EndDate})
</div>

<script type="text/javascript">

    $(document).ready(function () {
        $('.datepicker').not('.hasDatePicker').datepicker();

    });

 </script>